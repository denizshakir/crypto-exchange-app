{
  "rewrites": [
    {
      "source": "/api/binance/:path*",
      "destination": "https://api.binance.com/:path*"
    },
    {
      "source": "/api/kraken/:path*",
      "destination": "https://api.kraken.com/:path*"
    },
    {
      "source": "/api/bitfinex/:path*",
      "destination": "https://api-pub.bitfinex.com/:path*"
    },
    {
      "source": "/api/bitfinex/:path*",
      "destination": "https://api.huobi.pro/:path*"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
